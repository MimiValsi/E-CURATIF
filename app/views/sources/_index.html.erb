<%# -----  Alsace  ----- %>
<% qt = Info.where(source_id: 1).count %>
<% s = Source.find(1) %>

<% if (qt == 0) %>
  <% s %>
    <%= button_to s.name, s, class:"button-g float-start", :method => :get %>

<% elsif (qt >= 1 && qt < 5) %>
  <% s %>
    <%= button_to s.name, s, class:"button-y float-start", :method => :get %>

<% elsif (qt >= 5 && qt < 10) %>
  <% s %>
    <%= button_to s.name, s, class:"button-o float-start", :method => :get %>

<% else %>
  <% s %>
    <%= button_to s.name, s, class:"button-r float-start", :method => :get %>
<% end %>



